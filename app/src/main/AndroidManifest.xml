		android:icon="@drawable/icon"
		android:label="Campo Minado"
		android:theme="@android:style/Theme.Light.NoTitleBar">
		<activity
			android:name=".MainActivity"
			android:label="Campo Minado">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
